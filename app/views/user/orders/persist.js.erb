<% if @order.processing? %>
	<% if action_name == 'edit' %>
	swal({
		title: 'Success',
		text: "Youre order is complete. Please check your pending charges",
		type: 'success',
		confirmButtonText: 'Done',
		confirmButtonClass: 'btn btn-brown text-uppercase',
		buttonsStyling: false
	}).then(function (result) {
		if (result) {
			window.location = '<%= user_weekly_meals_path %>';
		}
	});
	<% else %>
	swal({
		title: 'Success',
		text: "Youre order is complete. Please check your pending charges",
		type: 'success',
		confirmButtonText: 'Done',
		confirmButtonClass: 'btn btn-brown text-uppercase',
		buttonsStyling: false
	});
	<% end %>
<% else %>
	swal({
		title: 'Oops...',
		text: "Something went wrong %>",
		type: 'error',
		confirmButtonText: 'Done',
		confirmButtonClass: 'btn btn-brown text-uppercase',
		buttonsStyling: false
	});
<% end %>