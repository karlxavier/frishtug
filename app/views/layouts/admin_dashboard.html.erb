<!DOCTYPE html>
<html lang="en-us">
  <head>
    <title>Frishtug</title>
    <%= render 'shared/favicons' %>
    <%= render 'shared/font_assets'%>
    <%= action_cable_meta_tag %>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= stylesheet_pack_tag 'application' %>
    <%= javascript_include_tag 'application' %>
    <%= javascript_pack_tag 'app' %>
    <%= javascript_pack_tag 'admin/stock_notifications' %>
    <%= yield :header_js %>
  </head>
  <body>
    <%= render 'admin/admin_navbar' %>
    <div class="container-fluid px-0" style="padding-top: 4.375rem;">
      <div class="row mx-0" style="overflow-x: hidden;display: grid;grid-template-columns: 200px 1fr 1fr 1fr 1fr 1fr;grid-template-rows: auto;grid-template-areas: 'sidebar body body body body body';">
        <%= render 'admin/admin_sidebar' %>
        <div class="col pr-0" style="grid-area: body;">
          <%= render 'shared/notifications' %>
          <%= yield %>
        </div>
      </div>
    </div>
    <%= yield :footer_js %>
    <%= javascript_include_tag 'https://use.fontawesome.com/69863dce90.js' %>
  </body>
</html>
