<!DOCTYPE html>
<html lang="en-us">
  <head>
    <title>Frishtug</title>
    <%= render 'shared/favicons' %>
    <%= render 'shared/font_assets'%>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= stylesheet_pack_tag 'application' %>
    <%= javascript_include_tag 'application' %>
    <%= javascript_pack_tag 'app' %>
    <%= include_uploadcare_widget_from_cdn version: "3.2.1", min: true %>
    <script> UPLOADCARE_PUBLIC_KEY = '8b856cdd0a320cbe8d19'; </script>
    <%= yield :header_js %>
  </head>
  <body>
    <%= render 'admin/admin_navbar' %>
    <div class="container-fluid mt-3">
      <div class="row">
        <%= render 'admin/admin_sidebar' %>
        <div class="col-12 col-md-9 col-xl-10">
          <%= render 'shared/notifications' %>
          <%= yield %>
        </div>
      </div>
    </div>
    <%= yield :footer_js %>
    <%= javascript_include_tag 'https://use.fontawesome.com/69863dce90.js' %>
  </body>
</html>
