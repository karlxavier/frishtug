<div class="form-group">
  <input type="text" name="registration_form[card_number]" id="registration_form_card_number" class="form-control" placeholder="Credit Card Number">
</div>
<div class="form-group row">
  <div class="col">
    <%= select "registration_form", "month", months_with_index_options, {}, class: 'form-control' %>
  </div>
  <div class="col">
    <%= select "registration_form", "year", years_options, {}, class: 'form-control' %>
  </div>
  <div class="col">
    <input type="number" min="0" max="9999" name="registration_form[cvc]" id="registration_form_cvc" class="form-control cvc-input-control" placeholder="CVC">
  </div>
</div>
<div class="form-check">
  <label for="registration_form_different_billing" class="form-check-label">
    <input type="checkbox" name="registration_form[different_billing]" value="true" class="form-check-input" id="registration_form_different_billing">
    Billing address is different to delivery address
  </label>
</div>
<div class="different-billing d-none">
  <div class="form-group">
    <input type="text" class="form-control" name="registration_form[billing_line_1]" placeholder="Address Line 1">
  </div>
  <div class="form-group">
    <input type="text" class="form-control" name="registration_form[billing_line_2]" placeholder="Address Line 2">
  </div>
  <div class="form-group row">
    <div class="col">
      <input type="text" class="form-control" name="registration_form[billing_city]" placeholder="City">
    </div>
    <div class="col">
      <input type="text" class="form-control" name="registration_form[billing_state]" placeholder="State">
    </div>
  </div>
  <div class="form-group row">
    <div class="col">
      <input type="text" class="form-control" name="registration_form[billing_zip_code]" placeholder="ZIP Code">
    </div>
    <div class="col">
      <input type="text" class="form-control" name="registration_form[billing_phone_number]" placeholder="Phone Number">
    </div>
  </div>
</div>